<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>Send_Something_To_Contact</fullName>
        <description>Sends the custom email template to the contact email</description>
        <protected>false</protected>
        <recipients>
            <field>ContactEmail</field>
            <type>email</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>My_Custom_Deployed_Templates/My_Email_Template</template>
    </alerts>
</Workflow>
